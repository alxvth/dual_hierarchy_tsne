{
  "name": "dual-hierarchy-tsne",
  "version-string": "0.1.0",
  "dependencies": [
    "date",
    {
      "name": "glad",
      "features": [ "gl-api-latest" ]
    },
    "glfw3",
    "glm",
    "indicators"
  ],
  "features": {
    "vis": {
      "description": "Build visualization library",
      "dependencies": [
        {
          "name": "imgui",
          "features": [ "opengl3-binding", "glfw-binding"] 
        }
      ]
    },
    "demo": {
      "description": "Build demo application",
      "dependencies": [
        "cxxopts"
      ]
    },
    "faiss" : {
      "description": "Use faiss for internal knn computation",
      "dependencies": [
        { "name": "faiss", "features": [ "gpu" ] }
      ]
    }
  }
}